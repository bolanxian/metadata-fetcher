{
  "imports": {
    "@/main.ssr": "./dist/main.ssr.js"
  },
  "permissions": {
    "default": {
      "read": ["."],
      "write": ["./__cache__"],
      "import": false,
      "env": true,
      "net": true,
      "run": true,
      "ffi": true,
      "sys": true
    },
    "start": {
      "read": true,
      "write": ["./__cache__"],
      "import": false,
      "env": true,
      "net": true,
      "run": true,
      "ffi": true,
      "sys": true
    }
  },
  "tasks": {
    "fetch:node": "node --import=./lib/preload.node.ts ./lib/main.ts fetch",
    "batch:node": "node --import=./lib/preload.node.ts ./lib/main.ts batch",
    "serve:bun": "bun run --preload=./lib/preload.bun.ts ./lib/main.ts serve",
    "fetch:bun": "bun run --preload=./lib/preload.bun.ts ./lib/main.ts fetch",
    "batch:bun": "bun run --preload=./lib/preload.bun.ts ./lib/main.ts batch",
    "serve": "deno run --quiet --no-prompt --no-remote -P ./lib/main.ts serve",
    "fetch": "deno run --quiet --no-prompt --no-remote -P ./lib/main.ts fetch",
    "batch": "deno run --quiet --no-prompt --no-remote -P ./lib/main.ts batch",
    "start": "deno run --quiet --no-prompt --no-remote -P=start ./lib/main.ts start"
  }
}